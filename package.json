{
  "name": "ack-angular-fx",
  "version": "1.3.0",
  "description": "Angular2 animations made easier",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "ack-angular-fx": "./bin/cli.js"
  },
  "scripts": {
    "watch": "ack-webpack example/index.ts example/dist/bundle.js --watch --browser=example/dist/ --project example/tsconfig.json --html5Mode",
    "watch:jit": "webpack-dev-server --config ./webpack.config.js --progress --profile --colors --watch --open",
    "build:example:jit": "webpack --config ./webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "build:example": "ack-webpack example/index.ts example/dist/bundle.js --production --project example/tsconfig.json",
    "test:prefx": "node ./bin/real-cli.js --igniter void --select absoluteSwap,100,childStag --effects slide --out test/results/prefx.ts",
    "compile:prefx": "npm-run-all test:prefx compile:example:prefx",
    "build:dist": "ngc --rootDir src --outDir dist --declaration --project src",
    "build": "npm run compile:example:prefx && npm run compile:example:aot && npm run build:example",
    "ts-node": "ts-node",
    "compile:example:prefx": "node ./bin/real-cli.js --igniter void --select absoluteSwap,100,200,300,400,500,600,700,800,900,1000,1500,2000,childStag --effects bounce --out example/src/app/prefx.ts",
    "compile:example:aot": "ngc -p ./example/tsconfig.aot.json",
    "ack-webpack": "ack-webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/ack-angular-fx.git"
  },
  "author": "Acker Dawn Apple",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@types/jasmine": "^2.6.2",
    "@types/node": "^8.0.47",
    "ack-webpack": "^1.1.51",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "web-animations-js": "^2.3.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "zone.js": "^0.8.18"
  },
  "jsDependencies": {
    "@angular/animations": "^5.0.0",
    "web-animations-js": "^2.3.1"
  },
  "bugs": {
    "url": "https://github.com/ackerapple/ack-angular-fx/issues"
  },
  "homepage": "https://github.com/ackerapple/ack-angular-fx#readme",
  "dependencies": {
    "ts-node": "^3.3.0",
    "minimatch": "^3.0.4"
  }
}
