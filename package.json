{
  "name": "ack-angular-fx",
  "version": "1.0.11",
  "description": "Angular2 animations made easier",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "ack-angular-fx": "./bin/cli.js"
  },
  "scripts": {
    "watch:example": "webpack-dev-server --config ./webpack.aot.config.js --progress --profile --colors --watch --open",
    "watch:example:jit": "webpack-dev-server --config ./webpack.config.js --progress --profile --colors --watch --open",
    "build:example:jit": "webpack --config ./webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "build:example": "webpack --config ./webpack.aot.config.js --progress --profile --colors --display-error-details --display-cached",
    "test:prefx": "node ./bin/real-cli.js --select absoluteSwap,100 --effects slide --out test/results/prefx.ts",
    "tsc:prefx": "tsc --rootDir scripts/prefx --outDir . --declaration --project scripts/prefx",
    "build:dist": "tsc --rootDir src --outDir dist --declaration --project src",
    "build":"npm run compile:example:prefx && npm run compile:example:aot && npm run build:example && npm run build",
    "ts-node": "ts-node",
    "compile:example:prefx": "node ./bin/real-cli.js --select absoluteSwap,100,200,300,400,500,600,700,800,900,1000,1500,2000 --out example/src/app/prefx.ts",
    "compile:example:aot": "ngc -p ./example/tsconfig.aot.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/ack-angular-fx.git"
  },
  "author": "Acker Dawn Apple",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.2.0",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "web-animations-js": "^2.2.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "zone.js": "^0.7.7"
  },
  "jsDependencies": {
    "web-animations-js": "^2.2.2"
  },
  "bugs": {
    "url": "https://github.com/ackerapple/ack-angular-fx/issues"
  },
  "homepage": "https://github.com/ackerapple/ack-angular-fx#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "ts-node": "^2.1.0"
  }
}
