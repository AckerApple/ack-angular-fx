{
  "name": "ack-angular-fx",
  "version": "2.0.0",
  "description": "Angular animations made easier",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "ack-angular-fx": "./bin/cli.js"
  },
  "scripts": {
    "test": "npm run test:prefx",
    "build:templates": "ack-pug-bundler example/src/app example/src/app --outFileExt template.ts --outType ts --oneToOne",
    "watch:templates": "npm run build:templates -- --watch",
    "watch:ts": "ack-webpack example/index.ts example/dist/bundle.js --watch --browser=example/dist/ --project example/tsconfig.json --html5Mode",
    "start": "ack-reload -d example/dist",
    "watch": "npm-run-all --parallel watch:templates watch:ts",
    "build:example": "npm-run-all build:templates build:example:js",
    "build:example:js": "ack-webpack example/index.ts example/dist/bundle.js --production --project example/tsconfig.json",
    "test:prefx": "node ./bin/real-cli.js --igniter void --select absoluteSwap,100,childStag --effects slide,fade --out test/results/prefx.ts",
    "compile:prefx": "npm-run-all test:prefx compile:example:prefx",
    "build:dist": "ngc --rootDir src --outDir dist --declaration --project src",
    "build": "npm-run-all build:dist build:bin compile:example:prefx compile:example:aot build:example",
    "build:bin": "tsc bin/src/* --outDir bin --declaration true --lib ES6",
    "ts-node": "ts-node",
    "compile:example:prefx": "node ./bin/real-cli.js --igniter void --select absoluteSwap,100,200,300,400,500,600,700,800,900,1000,1500,2000,childStag --effects bounce,fade --out example/src/app/prefx.ts",
    "compile:example:aot": "ngc -p ./example/tsconfig.aot.json",
    "ack-webpack": "ack-webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/ack-angular-fx.git"
  },
  "author": "Acker Dawn Apple",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.4.6",
    "ack-pug-bundler": "^1.3.15",
    "ack-webpack": "^1.2.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "zone.js": "^0.8.18"
  },
  "bugs": {
    "url": "https://github.com/ackerapple/ack-angular-fx/issues"
  },
  "homepage": "https://github.com/ackerapple/ack-angular-fx#readme",
  "dependencies": {
    "ts-node": "^4.1.0"
  }
}
